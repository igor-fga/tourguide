<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.tourguide">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:label="@string/restaurantes"
            android:name=".RestaurantActivity"
            />
        <activity
            android:label="@string/bares"
            android:name=".BarActivity" />
        <activity
            android:label="@string/baladas"
            android:name=".BaladasActivity"
            />
        <activity
            android:label="@string/passeios"
            android:name=".PasseiosActivity"
            />
    </application>

</manifest>